{
  "name": "unb-libraries/dockworker",
  "description": "Services for wiring up docker containers for development",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "asm89/twig-lint": "~1.0.2",
    "aws/aws-sdk-php": "~3.62.9",
    "consolidation/robo": "~1.4.9",
    "droath/robo-docker-compose": "~0.0.8",
    "ext-posix": "*",
    "j13k/yaml-lint": "~1.1.3",
    "leafo/scssphp": "~0.7.7",
    "nikic/php-parser": "~4.2.1",
    "sweetchuck/robo-phpcs": "~0.0.20"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "UNB Libraries",
      "email": "libsupport@unb.ca"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dockworker\\": "./src/"
    }
  },
  "bin": [
    "bin/dockworker"
  ],
  "scripts": {
    "dockworker-kubectl-init": "vendor/unb-libraries/dockworker/scripts/kubectl/init.sh",
    "dockworker-trav-init": "vendor/unb-libraries/dockworker/scripts/travis/init.sh"
  }
}
