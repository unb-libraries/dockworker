{
  "name": "unb-libraries/dockworker",
  "description": "Services for wiring up docker containers for development",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "asm89/twig-lint": "^1.0.2",
    "consolidation/robo": "^1.2.3",
    "droath/robo-docker-compose": "~0.0.8",
    "j13k/yaml-lint": "^1.1.3",
    "leafo/scssphp": "^0.7.5",
    "nikic/php-parser": "^4.2.0",
    "sweetchuck/robo-phpcs": "^0.0.19"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "UNB Libraries",
      "email": "libsupport@unb.ca"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dockworker\\": "./src/"
    }
  },
  "bin": [
    "bin/dockworker"
  ],
  "scripts": {
    "post-update-cmd": [
      "vendor/bin/dockworker git:setup-hooks"
    ]
  }
}
